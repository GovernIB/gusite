<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping package="org.ibit.rol.sac.micromodel">
	
	<class name="org.ibit.rol.sac.micromodel.RespuestaDato" table="GUS_ENCVOT">

		<meta attribute="sync-DAO">false</meta>

		<id name="Id" type="long" column="VOT_CODI">
			<generator class="sequence">
                <param name="sequence">GUS_SEQRDA</param>
            </generator>
		</id>

  		<property name="idencuesta" column="VOT_IDENCU" type="long" not-null="false" />	
		<property name="idpregunta" column="VOT_IDPREG" type="long" not-null="false" />
		<property name="idrespueta" column="VOT_IDRESP" type="long" not-null="false" />
		<property name="dato" column="VOT_INPUT" type="string" />
		<property name="idusuari" column="VOT_CODUSU" type="long" />

	</class>	

</hibernate-mapping>