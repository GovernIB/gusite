<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping package="org.ibit.rol.sac.micromodel">
	
	<class name="org.ibit.rol.sac.micromodel.Tiposervicio" table="GUS_TIPSER">

		<meta attribute="sync-DAO">false</meta>
		
		<id	name="Id" type="long" column="TPS_CODI">
			<generator class="sequence">
                <param name="sequence">GUS_SEQTIP</param>
            </generator>
		</id>

		<property name="nombre" column="TPS_NOMBRE" type="string" not-null="true" length="64" />
		<property name="visible" column="TPS_VISIB" type="string" not-null="true" length="1" />
		<property name="url" column="TPS_URL" type="string" not-null="true" length="64" />
		<property name="tipo" column="TPS_TIPO" type="string" not-null="true" length="1" />
		<property name="referencia" column="TPS_REF" type="string" not-null="true" length="5" />
	

	</class>	
</hibernate-mapping>