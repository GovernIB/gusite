<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="org.ibit.rol.sac.micromodel.UsuarioEncuesta" table="GUS_USUARIENC">
        <meta attribute="sync-DAO">false</meta>
        <id name="id" column="USE_CODI">
            <generator class="sequence">
                <param name="sequence">GUS_SEQUSUENC</param>
            </generator>
        </id>
        <property name="nombre" column="USE_NOMBRE" length="256"/>
        <property name="observaciones" column="USE_OBSERV" length="4000"/>
		<property name="dni" column="USE_DNI" length="4000"/>
    </class>
</hibernate-mapping>