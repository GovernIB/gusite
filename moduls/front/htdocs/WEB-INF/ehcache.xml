<ehcache>

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
        maxElementsInMemory="800"
        eternal="false"
        timeToLiveSeconds="600"
        overflowToDisk="false"
        />

    <!-- Caches del front -->
    <cache name="TemplateDataService.plantillasSite"
        maxElementsInMemory="500"
        eternal="86400"
        timeToLiveSeconds="600"
        overflowToDisk="false"/>

    <cache name="BaseViewController.uos"
        maxElementsInMemory="500"
        eternal="86400"
        timeToLiveSeconds="600"
        overflowToDisk="false"/>
    
</ehcache>